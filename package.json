      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "/build/*",
      "/src/*",
      "/public/*"
    ],
    "delay": 100
  },
  "proxy": "https://localhost:4000"
